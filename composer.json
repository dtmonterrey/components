{
	"name": "dtmonterrey/yii2-components",
	"description": "Components for yii 2",
	"minimum-stability" : "stable",
	"require": {
		"dtmonterrey/module-user-management" : "*",
		"webvimark/image": "^1.0.0",
		"webvimark/helpers": "^1.0.0"
	},
	"autoload": {
		"psr-4": {
			"dtmonterrey\\components\\": ""
		}
	},
	"repositories" : [
		{
			"type": "git",
			"url": "https://github.com/dtmonterrey/user-management",
			"name": "https://github.com/dtmonterrey/user-management"
		}
	],
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	}
}
